algoritmo "semnome"

var
dimensaoa,dimensaob,area,potencia:real

inicio
ESCREVA ("insira as dimensoes da primeira parede:")
leia (dimensaoa)
ESCREVA ("insira as dimensoes da segunda parede:")
leia (dimensaob)

area <- dimensaoa*dimensaob

potencia <- area * 18

escreval ("a area do comodo é:", area)
escreval ("a poencia necessaria é de:", potencia, "w")
fimalgoritmo
